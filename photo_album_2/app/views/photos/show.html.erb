<div class = "photo">
    <h3> <%= @photo.title %></h3>
    <div>Created on <%= @photo.date %> </div>
    <div> <%= image_tag @photo.file_location.url, size: "140x140",class: "img-thumbnail" %> </div>
    <div class = "photo_description"> <%= @photo.description %> </div>
    <br>
    <p>
    <%= link_to 'Edit', edit_photo_path(@photo) %>
    </p>
    <p>
    <%= link_to 'Back to index', photos_path %>
    </p>
</div>